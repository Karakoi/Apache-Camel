<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="excel" class="component.excel.ExcelComponent" scope="prototype">
		<constructor-arg ref="excelEndpoint"></constructor-arg>
	</bean>

	<bean id="excelEndpoint" class="component.excel.ExcelEndpoint" scope="prototype">
	<!--  	<property name="fileNameFrom" value="E://Документи/Eclipse for Java EE Projects/Camel/componentsData/excelData/read/Real4.xlsx"></property>
	-->
	<!--  	<property name="fileNameTo" value="E://Документи/Eclipse for Java EE Projects/Camel/componentsData/excelData/write/dataFromOPC.xlsx"></property>
		-->
		<property name="sheetNo" value="0"></property>
		<property name="rowNo" value="0"></property>
		<property name="cellNo" value="0"></property>
	</bean>

	<bean id="excelProcessor" class="component.excel.ExcelProcessor"></bean>

</beans>
