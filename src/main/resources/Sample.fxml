<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="407.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.Controller">
    <bottom>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <AnchorPane prefHeight="30.0" prefWidth="251.0" GridPane.rowIndex="2">
                    <children>
                        <Button fx:id="connectButton" layoutY="3.0" mnemonicParsing="false" onAction="#connect"
                                prefHeight="26.0" prefWidth="91.0" text="Connect"/>
                        <Button fx:id="disConnectButton" layoutX="91.0" layoutY="3.0" mnemonicParsing="false"
                                onAction="#disconnect" prefHeight="26.0" prefWidth="91.0" text="Disconnect"/>
                    </children>
                </AnchorPane>
            </children>
        </GridPane>
    </bottom>
</BorderPane>
